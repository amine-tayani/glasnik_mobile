{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ESNext",
    "allowJs": true,
    "isolatedModules": true,
    "incremental": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "Node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
